class Solution {
    public int minOperations(int[] nums) {
        int target =nums[0];
        int o=0;
        for(int i=0;i<nums.length-1;i++){
            target&=nums[i];
        }
        for(int n:nums){
            if(n!=target){
                return 1;
            }
        }
        return 0;
    }
}
