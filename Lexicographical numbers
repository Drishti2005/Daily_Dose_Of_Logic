class Solution {
    public List<Integer> lexicalOrder(int n) {
        List<Integer>ll=new ArrayList<>();
        counting(n,0,ll);
        return ll;
    }
    public static void counting(int n,int curr,List<Integer>ll){
        if(curr>n){
            return;
        }
        int i=0;
        if(curr==0){
            i=1;
        }
        if(curr>0){
            ll.add(curr);
        }
       
        for(;i<=9;i++){
            counting(n,curr*10+i,ll);
        }
    }
}
